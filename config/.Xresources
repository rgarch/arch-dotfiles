					xft:Symbola:pixelsize=14:antialias=true,\
					xft:NanumGothicCoding:pixelsize=14:antialias=true

URxvt*boldFont:		xft:Source Code Pro:pixelsize=14:antialias=true:bold,\
					xft:NanumGothicCoding:pixelsize=14:antialias=true:bold

URxvt*cursorUnderline:      	False
URxvt*url-select.underline: 	False
URxvt*borderless:           	1
URxvt*scrollBar:            	False
URxvt*scrollBar_right:      	False
URxvt*depth:                	32
URxvt*urgentOnBell:         	False
URxvt*shading:              	30
URxvt*termName:					rxvt-unicode-256color
URxvt*background:  				[100]#000000
URxvt*foreground:  				[100]#FFFFFF
URxvt*cursorColor:      		#0033FF
URxvt*letterSpace:  			-1


