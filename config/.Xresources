urxvt*font:			xft:SourceCodePro:pixelsize=14:antialias=true,\
					xft:DejaVuSansMono:pixelsize=14:antialias=true,\
					xft:NanumGothic:pixelsize=14:antialias=true

urxvt*boldFont:		xft:SourceCodePro:pixelsize=14:antialias=true:bold,\
					xft:DejaVuSansMono:pixelsize=14:antialias=true:bold,\
					xft:NanumGothic:pixelsize=14:antialias=true:bold

urxvt*cursorUnderline:      	False
urxvt*url-select.underline: 	False
urxvt*borderless:           	1
urxvt*scrollBar:            	False
urxvt*scrollBar_right:      	False
urxvt*depth:                	32
urxvt*urgentOnBell:         	False
urxvt*shading:              	30
urxvt*termName:					rxvt-unicode-256color
urxvt*background:  				[85]#000000
urxvt*foreground:  				[100]#FFFFFF
urxvt*cursorColor:      		#0033FF

Xft.dpi:		100
Xft.antialias:	true
Xft.rgba:		rgb
Xft.hinting:	true
Xft.hintstyle:	full

