URxvt*cursorUnderline:      false
URxvt*url-select.underline: false
URxvt*borderless:           1
URxvt*scrollBar:            false
URxvt*scrollBar_right:      false
URxvt*depth:                32
URxvt*urgentOnBell:         False
URxvt*background:           [75]#000000
URxvt*foreground:           [100]#FFFFFF
URxvt*tintColor:            grey
URxvt*shading:              30
URxvt*font:                 xft:SourceCodePro:pixelsize=13.6:antialias=false:hinting=true,\
							xft:DejaVuSansMono:pixelsize=13.6:antialias=false:hinting=true,\
							xft:NanumGothic:pixelsize=13.6:antialias=false:hinting=true

URxvt*boldFont: 			xft:SourceCodePro:Bold:pixelsize=13.6:antialias=false:hinting=true,\
							xft:DejaVuSansMono:Bold:pixelsize=13.6:antialias=false:hinting=true,\
							xft:NanumGothic:Bold:pixelsize=13.6:antialias=false:hinting=true

! Black
urxvt*color0:                 #000000
urxvt*color8:                 #555555
! Red
urxvt*color1:                 #DC322F
urxvt*color9:                 #FF5555
! Green
urxvt*color2:                 #55FF55
urxvt*color10:                #00AA00
! Yellow
urxvt*color3:                 #AA5500
urxvt*color11:                #FFFF55
! Blue
urxvt*color4:                 #3C62AD
urxvt*color12:                #5385E8
! Magenta
urxvt*color5:                 #B837DA
urxvt*color13:                #FD68EA
! Cyan
urxvt*color6:                 #00FFFF
urxvt*color14:                #2AA198
! White
urxvt*color7:                 #949494
urxvt*color15:                #ffffff
! Cursor
urxvt*cursorColor:            #FF00D2

URxvt.perl-ext-common:   default,matcher,vtwheel
URxvt.url-launcher:      /usr/bin/chromium
URxvt.matcher.button:    1
